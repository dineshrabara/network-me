{
    "name": "Network Me devtools",
    "version": "1.0.0",
    "version_name": "1.0.0",
    "description": "Chrome DevTools extension for debugging/Export Network Requests.",
    "manifest_version": 2,
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "page_action": {
        "default_popup": "pages/popup.html",
        "default_icon": {
            "16": "icons/16.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        }
    },
    "devtools_page": "pages/devtools.html",
    "background": {
        "scripts": [
            "scripts/background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "file:///*",
        "contextMenus",
        "storage",
        "activeTab",
        "declarativeContent"
    ]
}